  "events": {
    "load": [
      {
        "name": "totogen_db.getFeeArrangements",
        "parameters": [],
        "then": [
          {
            "name": "getFeeArrangementsForFeeArrangementResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Totogen.Models.TotogenDb.FeeArrangement>"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "totogen_db.getFeeStatuses",
        "parameters": [],
        "then": [
          {
            "name": "getFeeStatusesForFeeStatusResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Totogen.Models.TotogenDb.FeeStatus>"
          }
        ],
        "type": "invoke"
      },
